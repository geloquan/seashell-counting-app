<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    tools:context=".ImageResultActivity">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/none"
        android:text="num. of object"
        android:textSize="10dp"
        app:layout_constraintBottom_toTopOf="@+id/punaw_count"
        app:layout_constraintEnd_toEndOf="@+id/punaw_count" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/none"
        android:text="Confidence Level:"
        android:textSize="10dp"
        app:layout_constraintStart_toStartOf="@+id/punaw_label"
        app:layout_constraintTop_toBottomOf="@+id/punaw_label" />

    <ImageView
        android:id="@+id/logo"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginStart="35dp"
        android:layout_marginBottom="40dp"
        android:src="@drawable/qualt_icon"
        app:layout_constraintBottom_toTopOf="@+id/punaw_label"
        app:layout_constraintStart_toStartOf="@+id/punaw_label" />

    <ImageView
        android:id="@+id/seashell_logo"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentEnd="true"
        android:src="@mipmap/ic_seashell_v2"
        app:layout_constraintBottom_toBottomOf="@+id/logo"
        app:layout_constraintStart_toEndOf="@+id/logo" />

    <TextView
        android:id="@+id/punaw_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="228dp"
        android:text="Manila Clam"
        android:textColor="@color/dark"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/punaw_count"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginEnd="24dp"
        android:textColor="@color/dark"
        app:layout_constraintTop_toTopOf="@id/punaw_label"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="30dp" />

    <ProgressBar
        android:id="@+id/punaw_conf_bar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="306dp"
        android:layout_height="30dp"
        android:layout_marginStart="24dp"
        android:indeterminate="false"
        android:max="100"
        android:progressDrawable="@drawable/custom_progress_drawable"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10" />

    <TextView
        android:id="@+id/punaw_conf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginEnd="24dp"
        android:text="0%"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/punaw_conf_bar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/punaw_conf_bar" />

    <TextView
        android:id="@+id/greenshell_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="Green Mussel"
        android:textColor="@color/dark"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/punaw_conf_bar" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/none"
        android:text="Confidence Level:"
        android:textSize="10dp"
        app:layout_constraintStart_toStartOf="@+id/greenshell_label"
        app:layout_constraintTop_toBottomOf="@+id/greenshell_label" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/none"
        android:text="Confidence Level:"
        android:textSize="10dp"
        app:layout_constraintStart_toStartOf="@+id/punaw_label"
        app:layout_constraintTop_toBottomOf="@+id/punaw_label" />

    <TextView
        android:id="@+id/greenshell_count"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginEnd="24dp"
        android:textColor="@color/dark"
        app:layout_constraintTop_toTopOf="@id/greenshell_label"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="30dp" />

    <ProgressBar
        android:id="@+id/greenshell_conf_bar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="306dp"
        android:layout_height="30dp"
        android:layout_marginStart="24dp"
        android:indeterminate="false"
        android:max="100"
        android:progressDrawable="@drawable/custom_progress_drawable"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/greenshell_conf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginEnd="24dp"
        android:text="0%"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/greenshell_conf_bar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/greenshell_conf_bar" />

    <TextView
        android:id="@+id/tuway_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="Mud Clam"
        android:textColor="@color/dark"
        android:textSize="30dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/greenshell_conf_bar" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/none"
        android:text="Confidence Level:"
        android:textSize="10dp"
        app:layout_constraintStart_toStartOf="@+id/tuway_label"
        app:layout_constraintTop_toBottomOf="@+id/tuway_label" />

    <TextView
        android:id="@+id/tuway_count"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginEnd="24dp"
        android:textColor="@color/dark"
        app:layout_constraintTop_toTopOf="@id/tuway_label"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="30dp" />


    <ProgressBar
        android:id="@+id/tuway_conf_bar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="306dp"
        android:layout_height="30dp"
        android:layout_marginStart="24dp"
        android:indeterminate="false"
        android:max="100"
        android:progressDrawable="@drawable/custom_progress_drawable"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <TextView
        android:id="@+id/tuway_conf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginEnd="24dp"
        android:text="0%"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/tuway_conf_bar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tuway_conf_bar" />


    <TextView
        android:id="@+id/notshell_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="24dp"
        android:text="not shell"
        android:textStyle="italic"
        android:textColor="@color/dark"
        android:textSize="30dp"
        app:layout_constraintBottom_toTopOf="@+id/textView9"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/none"
        android:text="Confidence Level:"
        android:textSize="10dp"
        app:layout_constraintBottom_toTopOf="@+id/notshell_conf_bar"
        app:layout_constraintStart_toStartOf="@+id/notshell_label" />

    <TextView
        android:id="@+id/notshell_count"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginEnd="24dp"
        android:textColor="@color/dark"
        android:text="0"
        app:layout_constraintTop_toTopOf="@id/notshell_label"
        app:layout_constraintEnd_toEndOf="parent"
        android:textSize="30dp" />


    <ProgressBar
        android:id="@+id/notshell_conf_bar"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="306dp"
        android:layout_height="30dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="72dp"
        android:indeterminate="false"
        android:max="100"
        android:progressDrawable="@drawable/custom_progress_drawable"
        app:layout_constraintBottom_toTopOf="@+id/retry_btn"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/notshell_conf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginEnd="24dp"
        android:text="0%"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/notshell_conf_bar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/notshell_conf_bar" />


    <Button
        android:id="@+id/retry_btn"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@color/none"
        android:drawableStart="@drawable/retry"
        app:layout_constraintBottom_toBottomOf="@+id/home_button"
        app:layout_constraintEnd_toStartOf="@+id/image_result_save_btn"
        app:layout_constraintStart_toEndOf="@+id/home_button" />

    <Button
        android:id="@+id/image_result_save_btn"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginRight="25dp"
        android:background="@color/none"
        android:drawableStart="@drawable/savee"
        app:layout_constraintBottom_toBottomOf="@+id/home_button"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/home_button"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@color/none"
        android:layout_marginLeft="24dp"
        android:layout_marginBottom="45dp"
        android:drawableRight="@drawable/house"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/none"
        android:text="object name"
        android:textSize="10dp"
        app:layout_constraintBottom_toTopOf="@+id/punaw_label"
        app:layout_constraintStart_toStartOf="@+id/punaw_label" />

</androidx.constraintlayout.widget.ConstraintLayout>