<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AnalyticsActivity">

    <TextView
        android:id="@+id/punaw_label"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="128dp"
        android:text="Punaw"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:textSize="40sp" />

    <TextView
        android:id="@+id/punaw_count"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginEnd="24dp"
        app:layout_constraintTop_toTopOf="@id/punaw_label"
        app:layout_constraintEnd_toEndOf="parent"
        tools:textSize="40sp" />

    <TextView
        android:id="@+id/greenshell_label"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="Greenshell"
        app:layout_constraintTop_toBottomOf="@id/punaw_label"
        app:layout_constraintStart_toStartOf="parent"
        tools:textSize="40sp" />

    <TextView
        android:id="@+id/greenshell_count"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginEnd="24dp"
        app:layout_constraintTop_toTopOf="@id/greenshell_label"
        app:layout_constraintEnd_toEndOf="parent"
        tools:textSize="40sp" />

    <TextView
        android:id="@+id/tuway_label"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginStart="24dp"
        android:layout_marginTop="16dp"
        android:text="Tuway"
        app:layout_constraintTop_toBottomOf="@id/greenshell_label"
        app:layout_constraintStart_toStartOf="parent"
        tools:textSize="40sp" />

    <TextView
        android:id="@+id/tuway_count"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:layout_marginEnd="24dp"
        app:layout_constraintTop_toTopOf="@id/tuway_label"
        app:layout_constraintEnd_toEndOf="parent"
        tools:textSize="40sp" />

</androidx.constraintlayout.widget.ConstraintLayout>